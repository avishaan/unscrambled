{
  "name": "Unscrambled",
  "version": "0.6",
  "manifest_version": 2,
  "description": "Make it easy for dislexic people to read",
  "icons": {
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persisent": true
  },
  "web_accessible_resources": [
    "style.css",
    "fonts/*.*"
  ],
  "content_scripts": [ {
    "all_frames": true,
    "exclude_globs": [  ],
    "include_globs": [ "*" ],
    "js": ["./libs/jquery-2.1.1.min.js", "script.js" ],
    "css": ["style.css"],
    "matches": [ "http://*/", "https://*/", "https://*/*", "http://*/*" ],
    "run_at": "document_end"
  } ],
  "permissions": [ "tabs", "http://*/", "https://*/", "https://*/*", "http://*/*", "contextMenus" ]
}
